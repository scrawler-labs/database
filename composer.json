{
    "name": "scrawler/database",
    "description": "Arca ORM integration bridge for scrawler framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Scrawler\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "authors": [
        {
            "name": "ipranjal",
            "email": "pranjaltech10@gmail.com"
        }
    ],
    "require": {
        "scrawler/arca": "^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "pestphp/pest": "^3.2",
        "scrawler/http": "^2.0",
        "rector/rector": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
